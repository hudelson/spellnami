{
  "name": "spellnami",
  "private": true,
  "version": "1.0.0",
  "description": "A falling-word typing game built with Phaser 3 and TypeScript. Test your typing skills as words fall from the sky!",
  "type": "module",
  "keywords": [
    "game",
    "typing",
    "phaser",
    "typescript",
    "tetris",
    "word-game"
  ],
  "author": "Phil Hudelson",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hudelson/spellnami.git"
  },
  "homepage": "https://hudelson.github.io/spellnami",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write ."
  },
  "dependencies": {
    "canvas": "^3.1.0",
    "matter-js": "^0.20.0",
    "phaser": "^3.60.0",
    "random-words": "^2.0.1"
  },
  "devDependencies": {
    "@types/matter-js": "^0.19.8",
    "@types/node": "^20.11.28",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "terser": "^5.39.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vite-plugin-static-copy": "^0.17.0",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^1.3.1"
